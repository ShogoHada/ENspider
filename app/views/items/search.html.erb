<p>検索</p>
<%= form_tag(search_path,:method => 'get') do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :name => nil %>
<% end %>
<table class="table table-hover table-inverse">
  <!--検索対象モデルがUserの時 -->
    <tbody>
      <% @items.each do |item| %>
        <tr>
        <td><%= item.word %></td>
        <td><%= item.mean %></td>
        <td><%= item.level %></td>
        </tr>
      <% end %>
      
    </tbody> 
</table>